{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    },
    "production": {
      "plugins": ["transform-remove-console"]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-remove-console"
      ]
    }
  },
  "sourceMaps": true
}
